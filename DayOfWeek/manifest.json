{
    "manifest_version": 1,
    "version": 2,
    "type": "APP",
    "name": {
        "ja": "曜日プラグイン",
        "en": "Day of Week Plug-in",
        "zh": "Day of Week Plug-in"
    },
    "description": {
        "ja": "日付の曜日を表示するプラグインです。",
        "en": "Displays the day of the week for a date field.",
        "zh": "Displays the day of the week for a date field"
    },
    "icon": "image/icon.png",
    "desktop": {
        "js": [
            "https://js.kintone.com/momentjs/2.22.1/moment.min.js",
            "https://js.kintone.com/jquery/3.3.1/jquery.min.js",
            "https://js.kintone.com/jsrender/0.9.90/jsrender.min.js",
            "js/customize.js"
        ],
        "css": [
            "css/51-current-default.css"
        ]
    },
    "config": {
        "html": "html/config.html",
        "js": [
            "https://js.kintone.com/jquery/2.2.4/jquery.min.js",
            "https://js.kintone.com/jsrender/0.9.90/jsrender.min.js",
            "js/config.js"
        ],
        "css": [
            "css/51-current-default.css",
            "css/config.css"
        ],
        "required_params": [
            "dateField",
            "dayOfWeekField"
        ]
    }
}
